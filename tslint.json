{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            true,
            "spaces"
        ],
        "interface-name": false,
        "no-console": [
            true,
            "warning"
        ],
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}